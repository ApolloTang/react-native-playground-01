{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
    "arrow-body-style": [1, "as-needed"],
    "camelcase": [0, "never"],
    "react/prop-types": [1],
    "react/forbid-prop-types": [1],
    "no-void": [0],
    "quote-props": [1]
  }
}
